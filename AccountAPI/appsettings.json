{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "spring": {
    "application": {
      "name": "global"
    },
    "cloud": {
      "config": {
        "url": "http://localhost:8888"
      }
    }
  },
  "eureka": {
    "client": {
      "shouldRegisterWithEureka": true,
      "serviceUrl": "http://localhost:8761/eureka",
      "ValidateCertificates": false
    },
    "instance": {
      "appName": "accountservice",
      "hostName": "localhost",
      "port": "6002"
    }
  },
  "AllowedHosts": "*"
}
